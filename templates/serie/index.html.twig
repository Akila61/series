{% extends "layout.html.twig" %}

{% block title %}Liste des séries{% endblock %}

{% block body %}<h1>Liste de séries</h1>

    <a href="{{ path('series_new') }}">Ajouter une série</a>

    <ul>
        {% for serie in series %}
            <li>
                <a href="{{ path('series_details', {'id': serie.id}) }}"> {{ serie.title }}</a>

            </li>
        {% endfor %}
    </ul>



{% endblock %}



